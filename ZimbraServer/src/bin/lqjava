#!/bin/bash

source `dirname $0`/lqshutil || exit 1
lqsetvars liquid_home liquid_java_home

exec ${liquid_java_home}/bin/java \
     -client \
     -Dliquid.home=${liquid_home} \
     -Djava.library.path=${liquid_home}/lib \
     -Djava.ext.dirs=${liquid_java_home}/jre/lib/ext:${liquid_home}/lib \
     "$@"
