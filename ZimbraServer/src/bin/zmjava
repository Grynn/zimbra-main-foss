#!/bin/bash

source `dirname $0`/zmshutil || exit 1
zmsetvars zimbra_home liquid_java_home

exec ${liquid_java_home}/bin/java \
     -client \
     -Dliquid.home=${zimbra_home} \
     -Djava.library.path=${zimbra_home}/lib \
     -Djava.ext.dirs=${liquid_java_home}/jre/lib/ext:${zimbra_home}/lib \
     "$@"
