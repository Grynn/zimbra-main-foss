# Order is important.
#
# APPLICATION name executable "args" mode "label"
#
# mode = "normal"
#
# SERVICE name "label" appname monitor_port control_syntax
#
# CONTROL_SYNTAX name cmd "syntax"
# CONTROL_SYNTAX proto "protocol-type" (text, http, soap)
#
# commands: start, restart, stop, exit, drain, status
#	plus app specific stuff
#
# $nm, $ex, $args, $md, $label

# exit_status_error_mappings 
# log_message_alerts 
#

PORT 7777

CONTROL_SYNTAX liquidsyntax proto "text"
CONTROL_SYNTAX liquidsyntax start "START"
CONTROL_SYNTAX liquidsyntax restart "RESTART"
CONTROL_SYNTAX liquidsyntax stop "STOP"
CONTROL_SYNTAX liquidsyntax exit "EXIT"
CONTROL_SYNTAX liquidsyntax drain "DRAIN"
CONTROL_SYNTAX liquidsyntax status "STATUS"
CONTROL_SYNTAX liquidsyntax statuschange "STATUSCHANGE"
CONTROL_SYNTAX liquidsyntax events "EVENTS"
CONTROL_SYNTAX liquidsyntax addhost "ADDHOST"
CONTROL_SYNTAX liquidsyntax removehost "REMOVEHOST"
CONTROL_SYNTAX liquidsyntax updatecluster "UPDATECLUSTER"
CONTROL_SYNTAX liquidsyntax shutdown "SHUTDOWN"
CONTROL_SYNTAX liquidsyntax reload "RELOAD"
CONTROL_SYNTAX liquidsyntax newfetchref "NEWFETCHREF"
CONTROL_SYNTAX liquidsyntax getfetchref "GETFETCHREF"
