		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.emailLabel$></td>
			<td><div id="${id}_EMAIL" tabindex="500"></div></td>
		</tr>
		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.phoneLabel$></td>
			<td><div id="${id}_PHONE" tabindex="600"></div></td>
		</tr>
		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.imLabel$></td>
			<td><div id="${id}_IM" tabindex="700"></div></td>
		</tr>
		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.addressLabel$></td>
			<td><div id="${id}_ADDRESS" tabindex="800"></div></td>
		</tr>
		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.urlLabel$></td>
			<td><div id="${id}_URL" tabindex="900"></div></td>
		</tr>
		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.otherLabel$></td>
			<td><div id="${id}_OTHER" tabindex="1000"></div></td>
		</tr>
		<tr valign=top>
			<td class=rowLabel><$=ZmMsg.notesLabel$></td>
			<td style='padding-left:3'><div id="${id}_NOTES" tabindex="1100"></div></td>
