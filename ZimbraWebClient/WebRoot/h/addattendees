                                    <c:set var="curTm" value="${indx*30}"/>
                                    <td><div class="gridDiv halfHour ${curTm eq aStart ? ' start ' :''} ${curTm eq aEnd ? ' end ' :''} ${indx eq 0 ? ' firstDiv ' : ''} ${indx % 2 eq 1 ? ' anHour ' : ''}"/></td>
