                            <tr>
                                <c:forEach begin="0" end="24" step="1" var="indx">
                                    <td colspan="2" align="center"><div class="hourLblDiv"/>${indx % 12 eq 0 ? 12 : (indx > 12 ? indx - 12 : indx) }</td>
